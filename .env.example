# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database

# Session Configuration
SESSION_SECRET=your-secret-key-here

# Authentication Configuration (Replit Auth)
# Note: These variables are specific to Replit's OAuth system
# For Render deployment, you'll need to configure these in Render's dashboard
REPLIT_DOMAINS=your-render-domain.onrender.com
REPL_ID=your-replit-app-id
ISSUER_URL=https://replit.com/oidc

# Server Configuration
NODE_ENV=production
PORT=10000
